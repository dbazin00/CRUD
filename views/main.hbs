<table class="table">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">text</th>
      <th scope="col">done</th>
      <th scope="col">createdAt</th>
      <th scope="col">updatedAt</th>
    </tr>
  </thead>
  <tbody>
    {{#each allTodos}}
      <tr>
        <th scope="row">{{this.id}}</th>
        <td>{{this.text}}</td>
        <td>{{this.done}}</td>
        <td>{{this.createdAt}}</td>
        <td>{{this.updatedAt}}</td>
      </tr>
    {{/each}}

  </tbody>
</table>